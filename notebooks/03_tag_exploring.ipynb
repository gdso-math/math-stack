   "source": [
    "def tag_regex(subject):\n",
    "    subj_dict = {}\n",
    "    pattern = re.compile(\"<(.+?)>\")\n",
    "    for text in subject[\"Tags\"]:\n",
    "        tags = re.findall(pattern, text)\n",
    "        for tag in tags:\n",
    "                if tag not in subj_dict:\n",
    "                    subj_dict[tag] = 1\n",
    "                else:\n",
    "                    subj_dict[tag] += 1\n",
    "    return subj_dict"
   ]
